# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:347
msgid "B<-text> - adds text"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:349
msgid "B<-unicodefiletext> - adds text from Unicode file"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:351
msgid ""
"B<-defanimation> - adds default animation with ID specified by user.ID for "
"different phones are different."
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:353
msgid "B<-animation> - adds \"frames\" frames read from file1, file2, etc."
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:355
msgid ""
"B<-defsound> - adds default sound with ID specified by user. ID for "
"different phones are different."
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:357
msgid ""
"B<-tone10> - adds IMelody version 1.0 read from RTTL or other compatible file"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:359
msgid ""
"B<-tone10long> - IMelody version 1.0 saved in one of few SMS with UPI. "
"Phones compatible with UPI (like SonyEricsson phones) will read such "
"ringtone as one"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:361
msgid ""
"B<-tone12> - adds IMelody version 1.2 read from RTTL or other compatible file"
msgstr ""

#. type: Plain text
#: docs/user/gammu.1:363
msgid ""
"B<-tone12long> - IMelody version 1.2 saved in one of few SMS with UPI. "
"Phones compatible with UPI (like SonyEricsson phones) will read such "
"ringtone as one"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:365
msgid ""
"B<-toneSE> - adds IMelody in \"short\" form supported by SonyEricsson phones"
msgstr ""

#. type: Plain text
#: docs/user/gammu.1:367
msgid ""
"B<-toneSElong> - add SonyEricsson IMelody saved in one or few SMS with UPI"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:369
msgid "B<-variablebitmap> - bitmap in any size saved in one SMS"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:371
msgid ""
"B<-variablebitmaplong> - bitmap with maximal size 96x128 saved in one or few "
"sms"
msgstr ""

#. type: Plain text
#: docs/user/gammu.1:373
msgid "B<-fixedbitmap> - bitmap 16x16 or 32x32"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:375
msgid ""
"B<-protected> - all ringtones and bitmaps after this parameter (excluding "
"default ringtones and logos) will be \"protected\" (in phones compatible "
"with ODI like SonyEricsson products it won't be possible to forward them "
"from phone menu)"
msgstr ""

#. type: Plain text
#: docs/user/gammu.1:377 docs/user/gammu.1:512
msgid ""
"B<-16bit> - Gammu uses SMS headers with 16-bit numbers for saving linking "
"info in SMS (it means less chars available for user in each SMS)"
msgstr ""

#. type: Plain text
#: docs/user/gammu.1:388
#, no-wrap
msgid ""
"B<-format> lcrasbiut - last text will be formatted. You can use combinations of chars:\n"
"    l - left aligned\n"
"    c - centered\n"
"    r - right aligned\n"
"    a - large font\n"
"    s - small font\n"
"    b - bold font\n"
"    i - italic font\n"
"    u - underlined font\n"
"    t - strikethrough font\n"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:390
msgid ""
"I<Example:> gammu savesms EMS -text \"Greetings\" -defsound 1 -text \"from "
"Gammu\" -tone10 axelf.txt -animation 2 file1.bmp file2.bmp"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:392
msgid ""
"will create EMS sequence with text \"Greetings\" and default sound 1 and "
"text \"from Gammu\" and ringtone axelf.txt and 2 frame animation read from "
"(1'st frame) file1.bmp and (2'nd frame) file2.bmp"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:394
msgid ""
"I<Example:> gammu savesms EMS -protected 2 -variablebitmaplong ala.bmp -"
"toneSElong axelf.txt -toneSE ring.txt"
msgstr ""

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:396
msgid "ala.bmp and axelf.txt will be \"protected\""
msgstr ""

