"NumberUnicode = 002B0034003500320032003700300036003900340037\n"
"Name = \"\"\n"
"NameUnicode =\n"
"Text00 = 004100420043004400450046004700480049004A004B004C004D004E004F0050005100520053005400550056005700580059005A000A\n"
"Coding = Default\n"
