msgstr "Emissor da Indicação de mensagem MMS."

#. type: TP
#: docs/user/gammu-smsd-run.7:77
#, no-wrap
msgid "B<DECODED_1_MMS_TITLE>"
msgstr "B<DECODED_1_MMS_TITLE>"

#. type: Plain text
#: docs/user/gammu-smsd-run.7:81
msgid "title of MMS indication message."
msgstr "título de indicação de mensagem MMS"

#. type: TP
#: docs/user/gammu-smsd-run.7:82
#, no-wrap
msgid "B<DECODED_1_MMS_ADDRESS>"
msgstr "B<DECODED_1_MMS_ADDRESS>"

