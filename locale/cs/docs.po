# type: Plain text
#: docs/user/gammurc.5:219 docs/user/gammu-smsdrc.5:286
msgid "gammu-smsd(1), gammu(1), gammurc(5)"
msgstr "gammu-smsd(1), gammu(1), gammurc(5)"

