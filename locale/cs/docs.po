#: docs/user/gammu.1:951 docs/user/gammu.1:959 docs/user/gammu.1:968
#: docs/user/gammu.1:976 docs/user/gammu.1:985 docs/user/gammu.1:994
#: docs/user/gammu.1:1003 docs/user/gammu.1:1012 docs/user/gammu.1:1172
#: docs/user/gammu.1:1181 docs/user/gammu.1:1302 docs/user/gammu.1:1311
#: docs/user/gammu.1:1425 docs/user/gammu.1:1434 docs/user/gammu.1:1979
#: docs/user/gammu.1:1988 docs/user/gammu.1:1996 docs/user/gammu.1:2005
#: docs/user/gammu.1:2014 docs/user/gammu.1:2023 docs/user/gammu.1:2052
#: docs/user/gammu.1:2060 docs/user/gammu.1:2069 docs/user/gammu.1:2078
#: docs/user/gammu.1:2269 docs/user/gammu.1:2278
msgid ""
"For description of shared parameters see B<Common parameters for sendsms and "
"savesms>."
msgstr ""
"Popis společných parametrů naleznete v sekci B<Společné parametry pro "
"sendsms a savesms>."

# type: TP
#. type: TP
#: docs/user/gammu.1:261 docs/user/gammu.1:1972
#, no-wrap
msgid "B<savesms BOOKMARK>I< file location [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"
msgstr "B<savesms BOOKMARK>I< file location [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:265 docs/user/gammu.1:1976
msgid ""
"Read WAP bookmark from file created by B<backup> option and saves in Nokia "
"format as SMS"
msgstr ""
"Načte WAP záložky ze souboru vytvořeného příkazem B<backup> a uloží je ve "
"formátu Nokie jako SMS"

# type: TP
#. type: TP
#: docs/user/gammu.1:270 docs/user/gammu.1:1293
#, no-wrap
msgid "B<savesms CALENDAR>I< file location [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"
msgstr "B<savesms CALENDAR>I< file location [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"

#. type: Plain text
#: docs/user/gammu.1:276 docs/user/gammu.1:1299
msgid ""
"Read calendar note from file created by B<backup> option and saves in "
"VCALENDAR 1.0 format as SMS. The location identifies position of calendar "
"item to be read in backup file (usually 1, but can be useful in case the "
"backup contains more items)."
msgstr ""
"Načte poznámku kalendáře ze zálohy vytvořené parametrem B<backup> a uloží ho "
"do telefonu ve formátu VCALENDAR 1.0 jako SMS. Parametr location určuje "
"pozici, ze které bude načten kalendář ze souboru se zálohou (obvykle 1, ale "
"může být užitečné, pokud záloha obsahuje více položek)."

# type: TP
#. type: TP
#: docs/user/gammu.1:281 docs/user/gammu.1:953
#, no-wrap
msgid "B<savesms CALLER>I< file [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"
msgstr "B<savesms CALLER>I< file [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:284 docs/user/gammu.1:956
msgid "Save caller logo as sms (Nokia format - size 72x14)."
msgstr "Uloží logo volajícího jako SMS (formát pro Nokia - velikost 72x14)."

# type: TP
#. type: TP
#: docs/user/gammu.1:289
#, no-wrap
msgid "B<savesms EMS>I< [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num] [-unicode] [-16bit] [-format lcrasbiut] [-text text] [-unicodefiletext file] [-defsound ID] [-defanimation ID] [-tone10 file] [-tone10long file] [-tone12 file] [-tone12long file] [-toneSE file] [-toneSElong file] [-fixedbitmap file] [-variablebitmap file] [-variablebitmaplong file] [-animation frames file1 ...] [-protected number]>"
msgstr "B<savesms EMS>I< [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num] [-unicode] [-16bit] [-format lcrasbiut] [-text text] [-unicodefiletext file] [-defsound ID] [-defanimation ID] [-tone10 file] [-tone10long file] [-tone12 file] [-tone12long file] [-toneSE file] [-toneSElong file] [-fixedbitmap file] [-variablebitmap file] [-variablebitmaplong file] [-animation frames file1 ...] [-protected number]>"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:293
msgid ""
"Saves EMS sequence. All parameters after B<-unread> (like B<-defsound>) can "
"be used few times."
msgstr ""
"Uloží EMS zprávu. Všechny parametry uvedené za B<-unread> (například B<-"
"defsound>) mohou být použity opakovaně."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:295
msgid "B<-text> - adds text"
msgstr "B<-text> - přidá text"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:297
msgid "B<-unicodefiletext> - adds text from Unicode file"
msgstr "B<-unicodefiletext> - přidá text ze souboru v unicode"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:299
msgid ""
"B<-defanimation> - adds default animation with ID specified by user.ID for "
"different phones are different."
msgstr ""
"B<-defanimation> - přidá předdefinovanou animaci podle zadaného ID. Některé "
"telefony mohou podporovat více ID a ty nemusí odpovídat jiným značkám "
"telefonů."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:301
msgid "B<-animation> - adds \"frames\" frames read from file1, file2, etc."
msgstr ""
"B<-animation> - přidá \"frames\" snímků animace načtených ze souborů file1, "
"file2, atd."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:303
msgid ""
"B<-defsound> - adds default sound with ID specified by user. ID for "
"different phones are different."
msgstr ""
"B<-defsound> - přidá předdefinovaný zvuk podle zadaného ID. Některé telefony "
"mohou podporovat více ID a ty nemusí odpovídat jiným značkám telefonů."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:305
msgid ""
"B<-tone10> - adds IMelody version 1.0 read from RTTL or other compatible file"
msgstr ""
"B<-tone10> - přidá vyzvánění ve formátu iMelody verze 1.0 načtené z RTTL "
"nebo komatibilního souboru."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:307
msgid ""
"B<-tone10long> - IMelody version 1.0 saved in one of few SMS with UPI. "
"Phones compatible with UPI (like SonyEricsson phones) will read such "
"ringtone as one"
msgstr ""
"B<-tone10long> - přidá vyzvánění ve formátu iMelody verze 1.0 ve formátu s "
"UPI v několika SMS zprávách. Telefony podporující UPI (například Sony-"
"Ericsson) načtou takové vyzvánění jako jedno."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:309
msgid ""
"B<-tone12> - adds IMelody version 1.2 read from RTTL or other compatible file"
msgstr ""
"B<-tone12> - přidá vyzvánění ve formátu iMelody verze 1.2s načtené z RTTL "
"nebo komatibilního souboru."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:311
msgid ""
"B<-tone12long> - IMelody version 1.2 saved in one of few SMS with UPI. "
"Phones compatible with UPI (like SonyEricsson phones) will read such "
"ringtone as one"
msgstr ""
"B<-tone12long> - přidá vyzvánění ve formátu iMelody verze 1.2 ve formátu s "
"UPI v několika SMS zprávách. Telefony podporující UPI (například Sony-"
"Ericsson) načtou takové vyzvánění jako jedno."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:313
msgid ""
"B<-toneSE> - adds IMelody in \"short\" form supported by SonyEricsson phones"
msgstr ""
"B<-toneSE> - přidá vyzvánění ve formátu iMelody v \"krátké\" formě jak je "
"podporováno telefony Sony-Ericssonn."

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:315
msgid "B<-variablebitmap> - bitmap in any size saved in one SMS"
msgstr "B<-variablebitmap> - obrázek libovolné velikosti uložený v jedné SMS"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:317
msgid ""
"B<-variablebitmaplong> - bitmap with maximal size 96x128 saved in one or few "
"sms"
msgstr ""
"B<-variablebitmaplong> - obrázek o maximální velikosti 96x128 uložený v "
"jedné nebo více zpráv"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:319
msgid "B<-fixedbitmap > - bitmap 16x16 or 32x32"
msgstr "B<-fixedbitmap > - obrázek o velikosti 16x16 nebo 32x32"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:321
msgid ""
"B<-protected> - all ringtones and bitmaps after this parameter (excluding "
"default ringtones and logos) will be \"protected\" (in phones compatible "
"with ODI like SonyEricsson products it won't be possible to forward them "
"from phone menu)"
msgstr ""
"B<-protected> - všechna vyzvánění a obrázky (kromě předdefinovaných) uvedená "
"za tímto parametrem budou \"chráběna\" (v telefonech podporující standard "
"ODI jako například SonyEricsson je nebude možné přeposlat)"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:323
msgid ""
"I<Example:> gammu savesms EMS -text \"Greetings\" -defsound 1 -text \"from "
"Gammu\" -tone10 axelf.txt -animation 2 file1.bmp file2.bmp"
msgstr ""
"I<Příklad:> gammu savesms EMS -text \"Pozdrav\" -defsound 1 -text \"od Gammu"
"\" -tone10 axelf.txt -animation 2 file1.bmp file2.bmp"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:325
msgid ""
"will create EMS sequence with text \"Greetings\" and default sound 1 and "
"text \"from Gammu\" and ringtone axelf.txt and 2 frame animation read from "
"(1'st frame) file1.bmp and (2'nd frame) file2.bmp"
msgstr ""
"vytvoří EMS zprávu s textem \"Pozdrav\", předdefinovaným zvukem 1, textem "
"\"od Gammu\", vyzvánění axelf.txt a animací o dvou obrázcích složených ze "
"souborů (1. snímek) 1.bmp a (2. snímek) 2.bmp"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:327
msgid ""
"I<Example:> gammu savesms EMS -protected 2 -variablebitmaplong ala.bmp -"
"toneSElong axelf.txt -toneSE ring.txt"
msgstr ""
"I<Příklad:> gammu savesms EMS -protected 2 -variablebitmaplong ala.bmp -"
"toneSElong axelf.txt -toneSE ring.txt"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:329
msgid "ala.bmp and axelf.txt will be \"protected\""
msgstr "ala.bmp a axelf.txt budouch \"chráněné\""

# type: TP
#. type: TP
#: docs/user/gammu.1:334 docs/user/gammu.1:2042
#, no-wrap
msgid "B<savesms MMSINDICATOR>I< URL Title Sender [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"
msgstr "B<savesms MMSINDICATOR>I< URL Title Sender [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"

#. type: Plain text
#: docs/user/gammu.1:339 docs/user/gammu.1:2047
msgid ""
"Saves a message with MMS indication. The recipient phone will then download "
"MMS from given URL and display it."
msgstr ""
"Uloží zprávu s indikací MMS zprávy. Příjemcův telefon poté stáhe MMS se "
"zadané adresy a zobrazí ji."

#. type: Plain text
#: docs/user/gammu.1:341 docs/user/gammu.1:2049
msgid "Please note that you should provide valid smil data on that URL."
msgstr "Prosím uvědomte si, že na zadaném URL by měla být platná smil data."

# type: TP
#. type: TP
#: docs/user/gammu.1:346 docs/user/gammu.1:2054
#, no-wrap
msgid "B<savesms MMSSETTINGS>I< file location  [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"
msgstr "B<savesms MMSSETTINGS>I< file location  [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num]>"

#. type: Plain text
#: docs/user/gammu.1:350 docs/user/gammu.1:2058
msgid ""
"Saves a message with MMS configuration. The configuration will be read from "
"Gammu backup file from given location."
msgstr ""
"Uloží zprávu s nastavením MMS. Nastavení bude načteno ze zálohy Gammu ze "
"zadaného umístění."

# type: TP
#. type: TP
#: docs/user/gammu.1:354 docs/user/gammu.1:961
#, no-wrap
msgid "B<savesms OPERATOR>I< file [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num] [-netcode netcode] [-biglogo]>"
msgstr "B<savesms OPERATOR>I< file [-folder id] [-unread] [-read] [-unsent] [-sent] [-sender number] [-smsname name] [-smscset number] [-smscnumber number] [-reply] [-maxsms num] [-netcode netcode] [-biglogo]>"

# type: Plain text
#. type: Plain text
#: docs/user/gammu.1:358 docs/user/gammu.1:965
msgid ""
"Save operator logo as sms (Nokia format - size 72x14 or 78x21 after using B<-"
"biglogo>)."
msgstr ""
"Uloži logo operátora jako SMS (formát pro Nokie - velikost 72x14 nebo 78x21 "
"po použití B<-biglogo>)."

# type: TP
#. type: TP
#: docs/user/gammu.1:363 docs/user/gammu.1:970
#, no-wrap
