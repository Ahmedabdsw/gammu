msgstr "B<getfilefolder>I< ID1, ID2, ...>"

#. type: Plain text
#: docs/user/gammu.1:854
msgid ""
"Retrieve files or all files from folder with given IDs from a phone "
"filesytem."
msgstr ""
"Načte ze souborového systému telefonu soubory nebo všechny soubory ze složky "
"se zadaným ID."

# type: TP
#. type: TP
#: docs/user/gammu.1:854
#, no-wrap
msgid "B<getfiles>I< ID1, ID2, ...>"
msgstr "B<getfiles>I< ID1, ID2, ...>"

